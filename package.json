  "main": "index.js",
  "scripts": {
      "build": "tsc && npx copyfiles -u 1 src/generated/**/* dist",
      "build:force": "tsc || true && npx copyfiles -u 1 src/generated/**/* dist",
      "start": "node dist/index.js",
      "dev": "tsx watch --inspect --clear-screen=false src/index.ts",
      "migrate:dev": "prisma migrate dev",
      "lint:fix": "eslint . --fix",
      "format": "prettier . --write",
      "lint:nibble": "eslint-nibble",
      "test": "vitest",
      "test:run": "vitest run",
      "test:ui": "vitest --ui",
      "postinstall": "npx prisma generate",
      "import:csv": "tsx prisma/import.ts",
      "typecheck": "tsc -p tsconfig.typecheck.json --noEmit"
    },
  "prisma": {
    "seed": "tsx prisma/seed.ts",
    "schema": "prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
